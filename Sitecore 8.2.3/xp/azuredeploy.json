              "cdCoreSqlDatabaseUserName": "[parameters('cdCoreSqlDatabaseUserName')]",
              "cdCoreSqlDatabasePassword": "[parameters('cdCoreSqlDatabasePassword')]",
              "cdWebSqlDatabaseUserName": "[parameters('cdWebSqlDatabaseUserName')]",
