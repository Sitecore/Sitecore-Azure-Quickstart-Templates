          "opsWebAppName": {
            "value": "[parameters('opsWebAppName')]"
          },
          "opsWebAppFqdn": {
            "value": "[parameters('opsWebAppFqdn')]"
          },
          "authoringWebAppName": {
            "value": "[parameters('authoringWebAppName')]"
          },
          "authoringWebAppFqdn": {
            "value": "[parameters('authoringWebAppFqdn')]"
          },
          "minionsWebAppName": {
            "value": "[parameters('minionsWebAppName')]"
          },
          "minionsWebAppFqdn": {
