    "webSqlDatabaseName": {
      "type": "string",
      "minLength": 1,
      "defaultValue": "[coalesce(parameters('standard').webSqlDatabaseName, concat(parameters('deploymentId'), '-web-db'))]"
    },
    "shard0SqlDatabaseName": {
      "type": "string",
      "minLength": 1,
